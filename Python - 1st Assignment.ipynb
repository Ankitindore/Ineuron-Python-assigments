    "'hello'\n",
    "-87.8\n",
    "- \n",
    "/ \n",
    "+\t\n",
    "6 \n",
    "Ans : * is an Expressions\n",
    "      \"hello\" is Value\n",
    "      -87.8 is a Value\n",
    "      - is an Expressions\n",
    "      / is an Expressions\n",
    "      + is an Expressions\n",
    "      6 is a Value "
   ]
  },
